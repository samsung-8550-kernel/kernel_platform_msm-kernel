# CONFIG_TCP_CONG_HTCP is not set
CONFIG_DEFAULT_BIC=y
CONFIG_DEFAULT_TCP_CONG="bic"

# Performance
CONFIG_RQ_STAT_SHOW=y
CONFIG_SCHED_POWER_OPTIMIZE=y

# VOLD ASEC
CONFIG_CRYPTO_TWOFISH=m

